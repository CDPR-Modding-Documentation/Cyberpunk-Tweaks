QuickhackTable : LootTable
{
	minItemsToLoot = 1;
	maxItemsToLoot = 1;
	queries = 
	[
		{
			playerPrereqID = 
			{
				nestedPrereqs = 
				[
					{
						comparisonType = "Less";
					} : PlayerLevel_Tier_3_Start_Prereq, 
					{
						minimumQuality = 2;
					} : RarityOfEquippedCyberdeckPrereq
				];
			} : ORPrereq;
			dropChance = 0.60;
			query = "Query.UncommonQuickhackQuery";
		}, 
		{
			playerPrereqID = 
			{
				nestedPrereqs = 
				[
					"LootPrereqs.PlayerLevel_Tier_2_to_3_Exclusion_Prereq", {
						minimumQuality = 4;
					} : RarityOfEquippedCyberdeckPrereq
				];
			} : ORPrereq;
			dropChance = 0.60;
			query = "Query.RareQuickhackQuery";
		}, 
		{
			playerPrereqID = 
			{
				nestedPrereqs = 
				[
					"LootPrereqs.PlayerLevel_Tier_3_to_4_Exclusion_Prereq", {
						minimumQuality = 6;
					} : RarityOfEquippedCyberdeckPrereq
				];
			} : ORPrereq;
			dropChance = 0.60;
			query = "Query.EpicQuickhackQuery";
		}, 
		{
			playerPrereqID = 
			{
				nestedPrereqs = 
				[
					"LootPrereqs.PlayerLevel_Tier_4_to_5_Exclusion_Prereq", {
						minimumQuality = 8;
					} : RarityOfEquippedCyberdeckPrereq
				];
			} : ORPrereq;
			dropChance = 0.20;
			query = "Query.LegendaryQuickhackQuery";
		}
	];
}

