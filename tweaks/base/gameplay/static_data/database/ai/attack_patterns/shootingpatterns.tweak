{
	activationConditions = 
	[
		"ShootingPatterns.CloseRangePatternCondition"
	];
	delays = 
	[
		{
			shotNumber = 1;
			delay = 1.1f;
		} : AIPatternDelay, 
		{
			shotNumber = 2;
