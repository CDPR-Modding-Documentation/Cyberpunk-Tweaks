CloseRangePatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.TargetBelow15m"
		];
	} : AIActionAND;
}

MediumRangePatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.TargetBetween15and25m"
		];
	} : AIActionAND;
}

LongRangePatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.TargetAbove25m"
		];
	} : AIActionAND;
}

FullAutoPatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.RequestedTriggerModeFullAuto"
		];
	} : AIActionAND;
}

FullAutoSmartPatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.WeaponLockedOnTarget", "Condition.RequestedTriggerModeFullAuto"
		];
	} : AIActionAND;
}

SemiAutoPatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.RequestedTriggerModeSemiAuto"
		];
	} : AIActionAND;
}

BurstPatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.RequestedTriggerModeBurst"
		];
	} : AIActionAND;
}

ChargePatternCondition : AIActionCondition
{
	condition = 
	{
		AND = 
		[
			"Condition.RequestedTriggerModeCharge"
		];
	} : AIActionAND;
}

