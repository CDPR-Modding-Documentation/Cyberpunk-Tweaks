package RTDB

ProjectileCollision
{
	fk< ProjectileOnCollisionAction > type = "";
	float energyLossFactor = 0.f;
	bool canStopAndStickOnHardSurfaces = false;
}

Stop : ProjectileCollision
{
	type = "ProjectileOnCollisionActions.Stop";
}

Bounce : ProjectileCollision
{
	type = "ProjectileOnCollisionActions.Bounce";
}

StopAndStick : ProjectileCollision
{
	type = "ProjectileOnCollisionActions.StopAndStick";
}

StopAndStickPerpendicular : ProjectileCollision
{
	type = "ProjectileOnCollisionActions.StopAndStickPerpendicular";
}

Pierce : ProjectileCollision
{
	type = "ProjectileOnCollisionActions.Pierce";
}

