				{
					prereqRecord = 
					{
						statusEffect = "BaseStatusEffect.Haunted_Weapon_Kill_Human";
					} : StatusEffectPrereq;
					fact = "q305_haunted_weapon_npc_dead";
				} : ToggleFactEffector, 
				{
					prereqRecord = 
					{
						conditions += 
						[
							{
								npcType = "Human";
								invert = true;
							} : TargetNPCTypeHitPrereqCondition, 
							{
								hitFlag = "HauntedKill";
							} : HitFlagHitPrereqCondition
						];
					} : IsTargetKilled;
					statusEffect = "BaseStatusEffect.Haunted_Weapon_Kill_Mechanical";
				} : ApplyStatusEffectOnPlayer, 
				{
					prereqRecord = 
					{
						statusEffect = "BaseStatusEffect.Haunted_Weapon_Kill_Mechanical";
					} : StatusEffectPrereq;
					fact = "q305_haunted_weapon_android_dead";
				} : ToggleFactEffector, 
				{
					prereqRecord = 
					{
						statusEffect = "BaseStatusEffect.BlackwallHack_FactHelper";
					} : StatusEffectPrereq;
					fact = "q305_haunted_weapon_signature_hack";
				} : ToggleFactEffector
			];
		} : GameplayLogicPackage
	];
}

